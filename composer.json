{
    "name": "felicity-php/felicity-migrate",
    "version": "1.0.0",
    "description": "Migrations",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "TJ Draper",
            "email": "tj@buzzingpixel.com",
            "homepage": "https://buzzingpixel.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "felicity\\migrate\\": "src/"
        }
    },
    "require": {
        "roave/security-advisories": "dev-master",
        "felicity-php/felicity-config": "^1.0",
        "felicity-php/felicity-routing": "^1.0",
        "felicity-php/felicity-console-output": "^1.0",
        "felicity-php/felicity-translate": "^1.0",
        "felicity-php/felicity-architect": "^1.0"
    },
    "extra": {
        "bootstrap": [
            {
                "type": "directoryRecursive",
                "directoryPath": "src/translations"
            },
            {
                "type": "file",
                "filePath": "src/Bootstrap.php"
            }
        ]
    }
}
